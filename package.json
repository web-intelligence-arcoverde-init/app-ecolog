{
  "name": "Ecolog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "devices:ios": "xcrun xctrace list devices",
    "clean": "rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/Podfile.lock && rm -rf .temp && rm -rf yarn.lock && rm -rf package-lock.json && watchman watch-del-all",
    "build:android:dev:release": "cd ./android && ./gradlew assembleRelease",
    "build:android:dev:aab": "cd ./android && ./gradlew bundleRelease",
    "build:ios:prod:release": "cd ./ios && /usr/bin/xcodebuild -sdk iphoneos -configuration Prod -workspace smiles_app.xcworkspace -scheme smiles_app_prod build && /usr/bin/xcodebuild -exportArchive -archivePath output/iphoneos/Prod/archive/smiles_app.xcarchive -exportPath output/iphoneos/Prod/export -exportOptionsPlist ios/ExportOptions.plist",
    "build:ios:hml:release": "cd ./ios && /usr/bin/xcodebuild -sdk iphoneos -configuration Hml -workspace smiles_app.xcworkspace -scheme smiles_app_hml build && /usr/bin/xcodebuild -exportArchive -archivePath output/iphoneos/Hml/archive/smiles_app.xcarchive -exportPath output/iphoneos/Hml/export -exportOptionsPlist ios/ExportOptionsHom.plist"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "dependencies": {
    "@react-native-community/geolocation": "^3.0.1",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-redux": "^7.1.24",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "latinize": "^0.5.0",
    "lottie-react-native": "^5.1.4",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-device-info": "^10.2.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^13.2.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-swiper": "^1.6.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.4",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react-native": "^11.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/latinize": "^0.2.15",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
